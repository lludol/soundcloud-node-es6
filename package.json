{
  "name": "soundcloud-node-es6",
  "version": "0.0.1",
  "description": "A node.js wrapper for the Soundcloud SDK.",
  "main": "./out/main.js",
  "scripts": {
    "build": "babel src --presets babel-preset-es2015 --out-dir out",
    "cover": "istanbul cover _mocha",
    "coveralls": "npm run cover -- --report lcovonly && cat ./coverage/lcov.info | coveralls",
    "prepublish": "npm run build",
    "test": "mocha test/*.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lludol/soundcloud-node-es6.git"
  },
  "keywords": [
    "soundcloud",
	"soundcloud-node-es6",
    "node-soundcloud",
    "soundcloud-node",
    "music"
  ],
  "author": "lludol",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lludol/soundcloud-node-es6/issues"
  },
  "homepage": "https://github.com/lludol/soundcloud-node-es6#readme",
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-preset-es2015": "^6.9.0",
    "chai": "^3.5.0",
    "coveralls": "^2.11.11",
    "istanbul": "^0.4.4",
    "mocha": "^8.0.1",
    "mocha-lcov-reporter": "^1.2.0"
  },
  "dependencies": {
    "request": "^2.73.0"
  }
}
